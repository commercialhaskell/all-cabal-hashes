                       utf8-string >=0.3 && <0.4,
                       process >=1.1 && <1.3,
                       cmdargs >=0.10 && <0.11,
                       aeson >=0.7 && <0.8,
                       network >=2.4 && <2.5,
                       http-types >=0.8 && <0.9,
                       http-client >=0.2 && <0.3,
                       http-client-multipart >=0.2 && <0.3,
                       keyring >=0.1 && <0.2
  default-language:    Haskell2010
