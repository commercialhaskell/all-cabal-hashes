name:                data-extra
version:             2.0.0
synopsis:            Extra utilities for working on Data.* types.
description:         Extra utilities for working on Data.* types.
license:             BSD3
license-file:        LICENSE
author:              Chris Done
maintainer:          chrisdone@gmail.com
copyright:           2013 Chris Done
category:            Data
build-type:          Simple
cabal-version:       >=1.8

library
  exposed-modules:   Data.Extra,
                     Data.Either.Extra,
                     Data.Maybe.Extra,
                     Data.Bool.Extra,
                     Data.List.Extra,
                     Data.String.Extra,
                     Data.Text.Extra,
                     Data.Number.Extra,
                     Data.Time.Extra
  hs-source-dirs:    src/
  build-depends:     base > 4 && <5,
                     text,
                     old-locale,
                     time,
                     mtl
  ghc-options:       -Wall


