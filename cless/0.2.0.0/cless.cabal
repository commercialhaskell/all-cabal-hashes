  build-depends:       base >=4.7 && <4.8
                     , highlighting-kate >=0.5
                     , wl-pprint-terminfo >=3.7
                     , wl-pprint-extras
                     , terminfo
                     , optparse-applicative >=0.11
                     , process >=1.2

  hs-source-dirs:      src
  default-language:    Haskell2010
