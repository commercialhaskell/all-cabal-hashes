  other-extensions:    TypeOperators, FlexibleContexts, FlexibleInstances, ConstraintKinds, TypeFamilies, DataKinds
  build-depends:       base == 4.*, transformers >= 0.3, clean-unions
  hs-source-dirs:      src
  default-language:    Haskell2010

