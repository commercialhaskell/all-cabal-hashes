    Wumpus.Basic.Graphic.PointSupply,
    Wumpus.Basic.Monads.Drawing,
    Wumpus.Basic.Monads.DrawingMonad,
    Wumpus.Basic.Monads.DrawingCtxClass,
    Wumpus.Basic.Monads.TraceClass,
    Wumpus.Basic.Monads.TurtleClass,
