Source-Repository head
  type:         git
  location:     https://github.com/ghcjs/CodeMirror

library
  exposed-modules:  GHCJS.CodeMirror
  other-modules:    Paths_ghcjs_codemirror
  build-depends:    base <6
  default-language: Haskell2010
  hs-source-dirs:   src
