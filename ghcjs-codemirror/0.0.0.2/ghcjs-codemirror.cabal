    mode/meta.js
    mode/asterisk/asterisk.js
    mode/xml/xml.js
    mode/clike/clike.js
    mode/tcl/tcl.js
    mode/tiki/tiki.js
    mode/less/less.js
    mode/sql/sql.js
    mode/stex/stex.js
    mode/stex/test.js
    mode/coffeescript/coffeescript.js
    mode/python/python.js
    mode/diff/diff.js
    mode/go/go.js
    mode/scheme/scheme.js
    mode/jinja2/jinja2.js
    mode/smarty/smarty.js
    mode/sass/sass.js
    mode/http/http.js
    mode/gfm/gfm.js
    mode/gfm/test.js
    mode/shell/shell.js
    mode/haxe/haxe.js
    mode/turtle/turtle.js
    mode/rust/rust.js
    mode/apl/apl.js
    mode/markdown/markdown.js
