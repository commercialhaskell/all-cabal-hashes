  Ghc-options:       -Wall
                     -fno-warn-unused-do-bind
  Build-depends:     base >= 4 && < 5
                   , network
                   , parsec3
                   , bytestring
                   , transformers
                   , data-default
                   , template-haskell

  Other-modules:     Text.Config.TH
                   , Text.Config.Lib
                   , Text.Config.Parser
                   , Text.Config.Types

