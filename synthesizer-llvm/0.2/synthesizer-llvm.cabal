  If !flag(buildExamples)
    Buildable: False
  GHC-Options:    -Wall
  Hs-Source-Dirs: src
  Main-Is:     Synthesizer/LLVM/Test.hs

Executable synthi-llvm-server
  If !flag(buildExamples)
    Buildable: False
  GHC-Options:    -Wall -threaded
  Hs-Source-Dirs: src
  Main-Is:     Synthesizer/LLVM/Server.hs
  Other-Modules:
