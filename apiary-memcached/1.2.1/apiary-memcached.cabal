                     , memcached-binary   >=0.1   && <0.3
                     , data-default-class >=0.0   && <0.1
                     , bytestring         >=0.10  && <0.11
                     , text               >=1.1   && <1.3
                     , transformers       >=0.3   && <0.5
                     , monad-control      >=0.3   && <0.4
  hs-source-dirs:      src
  ghc-options:         -O2 -Wall
  default-language:    Haskell2010

