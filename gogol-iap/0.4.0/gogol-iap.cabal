name:                  gogol-iap
version:               0.4.0
synopsis:              Google Cloud Identity-Aware Proxy SDK.
homepage:              https://github.com/brendanhay/gogol
bug-reports:           https://github.com/brendanhay/gogol/issues
license:               OtherLicense
license-file:          LICENSE
author:                Brendan Hay
maintainer:            Brendan Hay <brendan.g.hay@gmail.com>
copyright:             Copyright (c) 2015-2016 Brendan Hay
category:              Network, Google, Cloud
build-type:            Simple
cabal-version:         >= 1.10
extra-source-files:    README.md src/.gitkeep

description:
    Controls access to cloud applications running on Google Cloud Platform.
    .
    /Warning:/ This is an experimental prototype/preview release which is still
    under exploratory development and not intended for public use, caveat emptor!
    .
    This library is compatible with version @v1@
    of the API.

source-repository head
    type:     git
    location: git://github.com/brendanhay/gogol.git

library
    default-language:  Haskell2010
    hs-source-dirs:    src gen

    ghc-options:       -Wall

    exposed-modules:
          Network.Google.IAP
        , Network.Google.IAP.Types
        , Network.Google.Resource.IAP.Projects.IapTunnel.GetIAMPolicy
        , Network.Google.Resource.IAP.Projects.IapTunnel.SetIAMPolicy
        , Network.Google.Resource.IAP.Projects.IapTunnel.TestIAMPermissions
        , Network.Google.Resource.IAP.Projects.IapTunnel.Zones.GetIAMPolicy
        , Network.Google.Resource.IAP.Projects.IapTunnel.Zones.Instances.GetIAMPolicy
        , Network.Google.Resource.IAP.Projects.IapTunnel.Zones.Instances.SetIAMPolicy
        , Network.Google.Resource.IAP.Projects.IapTunnel.Zones.Instances.TestIAMPermissions
        , Network.Google.Resource.IAP.Projects.IapTunnel.Zones.SetIAMPolicy
        , Network.Google.Resource.IAP.Projects.IapTunnel.Zones.TestIAMPermissions
        , Network.Google.Resource.IAP.Projects.IapWeb.GetIAMPolicy
        , Network.Google.Resource.IAP.Projects.IapWeb.Services.GetIAMPolicy
        , Network.Google.Resource.IAP.Projects.IapWeb.Services.SetIAMPolicy
        , Network.Google.Resource.IAP.Projects.IapWeb.Services.TestIAMPermissions
        , Network.Google.Resource.IAP.Projects.IapWeb.Services.Versions.GetIAMPolicy
        , Network.Google.Resource.IAP.Projects.IapWeb.Services.Versions.SetIAMPolicy
        , Network.Google.Resource.IAP.Projects.IapWeb.Services.Versions.TestIAMPermissions
        , Network.Google.Resource.IAP.Projects.IapWeb.SetIAMPolicy
        , Network.Google.Resource.IAP.Projects.IapWeb.TestIAMPermissions

    other-modules:
          Network.Google.IAP.Types.Product
        , Network.Google.IAP.Types.Sum

    build-depends:
          gogol-core == 0.4.0.*
        , base       >= 4.7 && < 5
