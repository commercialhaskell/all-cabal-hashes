 location: git://github.com/Daniel-Diaz/Clipboard.git

Library
  Exposed-modules: System.Clipboard
  Extensions: CPP
  if os(windows)
    Build-depends:  base == 4.*
                  , Win32 == 2.2.*
  else
    Build-depends:  base == 4.*
