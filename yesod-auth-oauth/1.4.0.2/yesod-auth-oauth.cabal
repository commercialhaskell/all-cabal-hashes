                   , yesod-core              >= 1.4       && < 1.5
                   , yesod-auth              >= 1.4       && < 1.5
                   , text                    >= 0.7
                   , yesod-form              >= 1.4       && < 1.5
                   , transformers            >= 0.2.2     && < 0.5
                   , lifted-base             >= 0.2       && < 0.3
    exposed-modules: Yesod.Auth.OAuth
    ghc-options:     -Wall

source-repository head
  type:     git
  location: https://github.com/yesodweb/yesod
