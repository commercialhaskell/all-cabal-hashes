name:                morphisms-functors
version:             0.1.5
synopsis:            Functors, theirs compositions and transformations
description:         Attempt to define categorical abstractions in more robust and useful way.
homepage:            https://github.com/iokasimov/morphisms-functors
license:             MIT
license-file:        LICENSE
extra-source-files:  CHANGELOG.md
author:              Murat Kasimov
maintainer:          Murat Kasimov <iokasimov.m@gmail.com>
copyright:           Copyright (c) 2018 Murat Kasimov
category:            Data, Control
build-type:          Simple
cabal-version:       >= 1.10

source-repository head
  type: git
  location: https://github.com/iokasimov/morphisms-functors.git

library
  build-depends: morphisms == 0.1.0
  exposed-modules:
    Control.Functor,
    Control.Variance,
    Control.Functor.Contravariant,
    Control.Functor.Covariant,
    Control.Functor.Invariant,
    Control.Functor.Covariant.Alternative,
    Control.Functor.Covariant.Applicative,
    Control.Functor.Covariant.Pointable,
    Control.Functor.Covariant.Extractable,
    Control.Functor.Covariant.Exclusive,
    Control.Functor.Covariant.Composition.Adjoint,
    Control.Functor.Covariant.Composition.Traversable,
    Control.Functor.Covariant.Composition.Distributive,
    Control.Functor.Covariant.Composition.Bindable,
    Control.Functor.Covariant.Composition.Extendable,
    Control.Functor.Covariant.Composition.Monad,
    Control.Functor.Covariant.Composition.Comonad,
    Control.Functor.Covariant.Transformation.Liftable,
    Control.Functor.Covariant.Transformation.Lowerable,
    Data.Functor,
    Data.Functor.Constant,
    Data.Functor.Identity,
    Data.Functor.Yoneda,
    Data.Functor.Composition.T,
    Data.Functor.Composition.TT,
    Data.Functor.Composition.TTT,
    Data.Functor.Transformation.Natural
  default-extensions:
    DataKinds
    ConstraintKinds
    FlexibleInstances
    FunctionalDependencies
    KindSignatures
    LiberalTypeSynonyms
    MultiParamTypeClasses
    NoImplicitPrelude
    PackageImports
    PolyKinds
    RankNTypes
    TypeFamilies
    TypeOperators
    TypeSynonymInstances
  default-language: Haskell2010
  ghc-options: -fno-warn-tabs
