Homepage: http://code.haskell.org/hexpat-pickle/
Extra-Source-Files:
  test/tests.hs,
  test/test.hs,
  test/test.xml,
  test/example.hs,
  test/lazyUnpickle.hs,
  test/lazyUnpickleThrow.hs
Build-Type: Simple
Stability: beta

Library
  Build-Depends:
    base >= 3 && < 5,
    hexpat >= 0.11,
    utf8-string >= 0.3.3,
    bytestring >= 0.9,
    text >= 0.1,
    containers,
    extensible-exceptions >= 0.1 && < 0.2
  Exposed-Modules: Text.XML.Expat.Pickle

