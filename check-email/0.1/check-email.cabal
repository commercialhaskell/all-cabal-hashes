  Exposed-Modules:  Network.Email.Check
  Ghc-options:      -Wall
  Extra-libraries:  gd, png, z, jpeg, m, fontconfig, freetype, expat
  Includes:         gd.h
  Include-dirs:     cbits
  Install-includes: check-mx.h
  C-sources:        cbits/check-mx.c
  Extra-libraries:  resolv
  Hs-source-dirs:   src


