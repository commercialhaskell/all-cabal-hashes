name:       toml
version:    0.1.3
cabal-version: >= 1.2
build-type: Simple
license: BSD3
description:
    Whiskey-driven YAML replacement.

library
    hs-source-dirs: src
    build-depends:
        base >= 4 && < 5,
        attoparsec >= 0.10 && < 0.13,
        containers >= 0.5 && < 0.6,
        bytestring >= 0.10 && < 0.11,
        time -any,
        old-locale -any

    exposed-modules:
        Text.TOML
        Text.TOML.Parser
        Text.TOML.Value


