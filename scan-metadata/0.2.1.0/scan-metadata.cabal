cabal-version:       2.2

name:                scan-metadata
version:             0.2.1.0
synopsis:            Metadata types for Albedo Scanners
description:         Metadata types for Albedo Scanners.
-- bug-reports:
license:             MIT
license-file:        LICENSE
author:              alexeyraga
maintainer:          alexey.raga@gmail.com
-- copyright:
category:            Data
extra-source-files:  README.md

source-repository head
  type: git
  location: https://github.com/packetloop/scan-metadata

library
  exposed-modules:
    Arbor.Albedo.Scan.Metadata
  -- other-modules:
  -- other-extensions:
  build-depends:
      base >=4.7 && <=5
    , aeson
    , antiope-core >=7.0.1
    , antiope-s3 >=7.0.1
    , bytestring
    , containers
    , generic-lens >= 1.1.0.0
    , hw-aeson >=0.1.0.1
    , hw-ip >=2.3.1.0
    , lens >= 4.17.1
    , mtl
    , text
    , thyme >=0.3.5.5
    , unliftio-core
  hs-source-dirs:      src
  default-language:    Haskell2010
