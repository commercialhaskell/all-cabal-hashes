    Canteven.HTTP
  -- other-modules:       
  -- other-extensions:    
  build-depends:
    base         >= 4.7     && < 4.10,
    bytestring   >= 0.10    && < 0.11,
    canteven-log >= 1.0     && < 2.1,
    exceptions   >= 0.8.2   && < 0.9,
    http-types   >= 0.9.1   && < 0.10,
    monad-logger >= 0.3.19  && < 0.4,
    text         >= 1.2     && < 1.3,
    time         >= 1.5.0.1 && < 1.7,
    transformers >= 0.4.2.0 && < 0.6,
    uuid         >= 1.3.12  && < 1.4,
    wai          >= 3.2     && < 3.3,
    wai-extra    >= 3.0.8   && < 3.1
  hs-source-dirs:      src
  default-language:    Haskell2010
  ghc-options: -Wall
