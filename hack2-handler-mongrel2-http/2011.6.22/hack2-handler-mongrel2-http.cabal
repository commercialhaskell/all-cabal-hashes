                    , directory
                    , safe
                    , text
                    , aeson
                    , blaze-textual
                    , unix
                    , attoparsec
  hs-source-dirs: src/
  exposed-modules:  
                    Hack2.Handler.Mongrel2HTTP
  other-modules:
                    Hack2.Handler.Mongrel2
                    Hack2.Handler.Mongrel2.Parser
                    Hack2.Handler.Mongrel2.Response
                    Hack2.Handler.Mongrel2.Types
                    


