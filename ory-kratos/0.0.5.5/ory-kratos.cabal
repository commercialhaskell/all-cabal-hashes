cabal-version: 3.0
name:          ory-kratos
version:       0.0.5.5
synopsis:      API bindings for Ory Kratos
description:   Auto-generated API bindings for ory-kratos 0.5.5-alpha1
homepage:      https://github.com/njaremko/ory-kratos-haskell-sdk
author:        ory
maintainer:    nathan@jaremko.ca
copyright:     2021 - Ory
category:      Web
build-type:    Simple
license:       Apache-2.0

library
  hs-source-dirs:   lib
  exposed-modules:
    OryKratos.API
    OryKratos.Types

  ghc-options:      -Wall
  build-depends:
    , aeson                ^>=1.5.6
    , base                 ^>=4.14.0.0
    , containers           ^>=0.6.2
    , exceptions           ^>=0.10.4
    , http-api-data        ^>=0.4.2
    , http-client          ^>=0.6.4
    , http-client-tls      ^>=0.3.5
    , http-types           ^>=0.12.3
    , mtl                  ^>=2.2.2
    , network-uri          ^>=2.6.4
    , servant              ^>=0.18.2
    , servant-client       ^>=0.18.2
    , servant-client-core  ^>=0.18.2
    , servant-server       ^>=0.18.2
    , swagger2             ^>=2.6
    , text                 ^>=1.2.4
    , time                 ^>=1.9.3
    , transformers         ^>=0.5.6
    , uuid                 ^>=1.3.14
    , wai                  ^>=3.2.3
    , warp                 ^>=3.3.14

  default-language: Haskell2010
