  x-uses-tf:       true
  build-depends:   
                   base >= 4,
                   HUnit >= 1.2 && < 2,
                   test-framework >= 0.4.1,
                   test-framework-hunit,
                   couchdb-conduit,
                   aeson >= 0.6 && < 0.7,
                   attoparsec >= 0.8 && < 0.11,
                   attoparsec-conduit >= 0.5 && < 0.6,
                   blaze-builder >= 0.2.1 && < 0.4,
                   bytestring >= 0.9 && < 0.10,
                   conduit >= 0.5 && < 0.6,
                   containers >= 0.2,
                   data-default,
                   http-conduit >= 1.5 && < 1.6,
                   http-types >= 0.7 && < 0.8,
                   lifted-base >= 0.1 && < 0.2,
                   monad-control >= 0.3 && < 0.4,
                   string-conversions,
                   syb,
                   transformers >= 0.2 && < 0.4,
                   transformers-base >= 0.4 && < 0.5,
                   text >= 0.11 && < 0.12,
                   unordered-containers >= 0.1
                   
  ghc-options:     -Wall -rtsopts -threaded
  hs-source-dirs:  test
  main-is:         Main.hs
  other-modules:   
                   CouchDBAuth,
