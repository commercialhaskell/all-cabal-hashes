    build-depends: 
        base >= 4 && < 5,
        unordered-containers,
        bytestring,
        music-score,
        music-preludes,
        aeson
    hs-source-dirs: src
    default-language: Haskell2010
    exposed-modules:
        Music.Sibelius
        Music.Score.Import.Sibelius

