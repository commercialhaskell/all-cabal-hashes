name:                python-pickle
version:             0.1.0
Cabal-Version:       >= 1.8
synopsis:            Serialization/deserialization using Python Pickle format.
description:
  This package implements serialization and deserialization of Python objects
  using the Pickle format.
category:            Development
license:             BSD3
license-file:        LICENSE
author:              Vo Minh Thu
maintainer:          thu@hypered.be
build-type:          Simple

source-repository head
  type: git
  location: https://github.com/noteed/python-pickle

library
  build-depends:       attoparsec == 0.10.*,
                       base == 4.*,
                       bytestring == 0.9.*,
                       cereal == 0.3.*,
                       containers == 0.4.*,
                       mtl == 2.0.*
  exposed-modules:     Language.Python.Pickle
  ghc-options:         -Wall

executable pickle
  main-is:             pickle.hs
  hs-source-dirs:      bin/
  build-depends:       base == 4.*,
                       bytestring == 0.9.*,
                       python-pickle
  ghc-options:         -Wall
