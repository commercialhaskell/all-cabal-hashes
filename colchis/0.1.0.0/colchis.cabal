        pipes-attoparsec == 0.5.*,
        pipes-aeson >= 0.4 && < 0.5,
        pipes-network >= 0.6 && < 0.7,
        aeson >=0.7 && < 0.9,
        text >= 0.11.2 && < 1.2,
        void >= 0.6 && < 0.7,
        conceit >= 0.1 && < 0.2,
        network

