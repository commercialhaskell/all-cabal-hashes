  hs-source-dirs:
      examples
  ghc-options: -Wall
  build-depends:
      base >= 4.8 && < 5
    , aeson >= 0.11
    , aeson-pretty >= 0.8
    , bytestring
    , hashable
    , lens >= 4.5
    , template-haskell >= 2.0
    , text >= 1.1
    , unordered-containers >= 0.2
    , stratosphere
  if flag(library-only)
    buildable: False
  default-language: Haskell2010

