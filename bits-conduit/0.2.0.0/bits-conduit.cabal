name:                bits-conduit
version:             0.2.0.0
synopsis:            Bitstream support for Conduit
description:         Bitstream support for Conduit
license:             BSD3
license-file:        LICENSE
author:              Hideyuki Tanaka
maintainer:          Hideyuki Tanaka <tanaka.hideyuki@gmail.com>
copyright:           (c) 2012, Hideyuki Tanaka
category:            Data, Conduit
build-type:          Simple
cabal-version:       >=1.8

source-repository head
  type:                git
  location:            https://github.com/tanakh/bits-conduit.git

library
  exposed-modules:     Data.Conduit.Bits

  build-depends:       base       ==4.5.*
                     , mtl        >=2.1
                     , bytestring >=0.9.2
                     , conduit    ==0.5.*

test-suite bits-conduit-test
  type:                exitcode-stdio-1.0
  hs-source-dirs:      dist
  main-is:             ../test.hs

  build-depends:       base
                     , mtl
                     , bytestring
                     , conduit

                     , hspec
                     , HUnit
                     , bits-conduit

benchmark bits-conduit-bench
  type:                exitcode-stdio-1.0
  hs-source-dirs:      dist
  main-is:             ../bench.hs

  build-depends:       base
                     , bytestring
                     , conduit
                     , random

                     , criterion >=0.6
                     , bits-conduit


