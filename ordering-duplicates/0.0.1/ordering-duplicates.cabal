  build-depends:        base                 >= 4.9 && < 4.15
                      , contravariant        == 1.5.5
                      , lens                 == 4.19.2
                      , mtl                  == 2.2.2

  hs-source-dirs:     src
  default-language:   Haskell2010
  ghc-options:        -Wall
