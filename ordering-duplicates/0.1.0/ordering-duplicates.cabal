name:                 ordering-duplicates
version:              0.1.0
synopsis:             Functions for detecting duplicates
description:          Functions for detecting duplicates in a list
license:              BSD3
license-file:         LICENCE
author:               Tony Morris <ʇǝu˙sıɹɹoɯʇ@ןןǝʞsɐɥ>
maintainer:           Tony Morris <ʇǝu˙sıɹɹoɯʇ@ןןǝʞsɐɥ>
copyright:            Copyright (C) 2023 Tony Morris
category:             Test
build-type:           Simple
extra-source-files:   changelog.md
cabal-version:        >=1.10
homepage:             https://gitlab.com/tonymorris/ordering-duplicates
bug-reports:          https://gitlab.com/tonymorris/ordering-duplicates/issues
tested-with:          GHC == 8.6.5, GHC == 9.0.2

source-repository     head
  type:               git
  location:           git@gitlab.com:tonymorris/ordering-duplicates.git

library
  exposed-modules:
                      Data.Ord.Ordering

  build-depends:        base                 >= 4.9 && < 6
                      , contravariant        >= 1.5.5 && < 2
                      , lens                 >= 4.19.2 && < 6
                      , mtl                  >= 2.2.2 && < 3
                      , semigroupoids        >= 5 && <= 7
                      , profunctors          >= 5 && <= 7

  hs-source-dirs:     src
  default-language:   Haskell2010
  ghc-options:        -Wall
