library 
  build-depends:  base < 5, mtl
  ghc-options:      -Wall
  exposed-modules:  Control.Monad.Trans.State.Plus

test-suite Main
  type:            exitcode-stdio-1.0
  build-depends:   base < 5, QuickCheck >= 2.7 && < 2.8, mtl, state-plus
                            , checkers >= 0.4.1 && < 0.5
  ghc-options:     -Wall
  hs-source-dirs:  tests
  main-is:         Main.hs



