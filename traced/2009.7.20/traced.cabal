    Exposed-modules:    Debug.Traced Debug.TracedInternal
    Other-modules:      Debug.StableMap
    
    Build-Depends:      mtl, pretty, containers
    
    if flag(base4)
        Build-Depends:  base >= 4 && < 5
    else
        Build-Depends:  base >= 3 && < 4