Library
  default-language: Haskell2010
  hs-source-dirs:   src
  
  build-depends:
    base     >= 4.12    && < 5,
    sdp      >= 0.2     && < 1,
    hashable >= 1.1.1.0 && < 1.4
  
  ghc-options: -Wall -Wno-orphans
  
  exposed-modules:
    SDP.Hashable



