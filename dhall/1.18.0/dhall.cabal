      Build-Depends: transformers == 0.4.2.*
      Build-Depends: fail == 4.9.*

    Exposed-Modules:
        Dhall,
        Dhall.Binary,
        Dhall.Context,
        Dhall.Core,
        Dhall.Diff,
        Dhall.Format,
        Dhall.Freeze,
        Dhall.Hash,
        Dhall.Import,
