name:                snap-testing
version:             0.4.1.0
synopsis:            A library for BDD-style testing with the Snap Web Framework
homepage:            https://github.com/dbp/snap-testing
license:             BSD3
license-file:        LICENSE
author:              Daniel Patterson
maintainer:          dbp@dbpmail.net
-- copyright:
category:            Web, Snap
build-type:          Simple
cabal-version:       >=1.8

library
  exposed-modules:
        Snap.Test.BDD
  hs-source-dirs: src
  -- other-modules:
  build-depends:       base ==4.6.*,
                       QuickCheck == 2.*,
                       snap == 0.13.*,
                       snap-core == 0.9.*,
                       mtl >= 2 && < 3,
                       transformers == 0.3.*,
                       text == 0.11.*,
                       bytestring >= 0.9.1 && < 0.11,
                       containers == 0.5.*,
                       process == 1.1.*,
                       io-streams == 1.*,
                       async == 2.*,
                       digestive-functors


