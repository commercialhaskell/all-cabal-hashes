name:           radium
version:        0.1
cabal-version:  >= 1.10
build-type:     Simple
author:         Krzysztof Langner
maintainer:     klangner@gmail.com
synopsis:       Chemistry
homepage:       https://github.com/klangner/radium
Bug-reports:    https://github.com/klangner/radium/issues
stability:      Unstable interface, incomplete features.
category:       Chemistry
License:        BSD3
License-file:   LICENSE
Extra-Source-Files:
                CHANGES
description:    
    This library simplifies writing application for chemistry. Currently includes:
    .
    * Element - Periodic table and basic information about elements

source-repository head
  type:     git
  location: https://github.com/klangner/radium

library
  hs-source-dirs:   src
  default-language: Haskell2010
  ghc-options:      -Wall
  build-depends:    
                    base >= 4 && <4.7
  exposed-modules:  
                    Chemistry.Element,
                    Chemistry.Molecule
  other-modules:    

test-suite unit-tests
  type:             exitcode-stdio-1.0
  main-is:          Spec.hs
  default-language: Haskell2010
  build-depends:   
                    base >= 4 && <4.7,
                    hspec >=1.8 && <1.9,
                    QuickCheck >=2.6 && <2.7,
                    Cabal >=1.16.0 && <1.17
  hs-source-dirs:  
                    src,
                    test-src


