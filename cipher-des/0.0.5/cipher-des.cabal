                   , bytestring
                   , QuickCheck >= 2
                   , test-framework >= 0.3.3
                   , test-framework-quickcheck2 >= 0.2.9
                   , byteable
                   , crypto-cipher-types
                   , crypto-cipher-tests
                   , cipher-des

