cabal-version:      >= 1.10
build-type:         Simple
extra-source-files: changelog

source-repository   head
  type:             git
  location:         git@github.com:tonymorris/casr-logbook.git

library
  default-language:
                    Haskell2010

  build-depends:
                    base >= 4.8 && < 5
                    , containers >= 0.4 && < 0.7
                    , lucid >= 2.9 && < 3
                    , digit >= 0.8 && < 1
                    , time >= 1.5 && < 1.13
                    , lucid >= 2.9
                    , lens >= 4.1 && < 6
                    , text >= 1.2 && < 3
                    , attoparsec >= 0.13 && < 0.15
  ghc-options:
                    -Wall

  default-extensions:
                    NoImplicitPrelude

  hs-source-dirs:
                    src

