Name:                clckwrks-plugin-ircbot
Version:             0.6.17.5
Synopsis:            ircbot plugin for clckwrks
Description:         At the moment, this ircbot just provides channel logs
Homepage:            http://www.clckwrks.com/
License:             BSD3
License-file:        LICENSE
Author:              Jeremy Shaw
Maintainer:          Jeremy Shaw <jeremy@n-heptane.com>
Copyright:           2012 Jeremy Shaw, SeeReason Partners LLC
Category:            Clckwrks
Build-type:          Custom
Cabal-version:       >=1.10
tested-with:         GHC == 7.8.4, GHC == 7.10.3, GHC == 8.0.1, GHC == 8.6.3
Data-Files:
    data/style.css

source-repository head
    type:     git
    location: git://github.com/clckwrks/clckwrks-plugin-ircbot.git

Library
  Default-Language: Haskell2010
  Build-tools:
    hsx2hs

  Exposed-modules:
    Clckwrks.IrcBot
    Clckwrks.IrcBot.Acid
    Clckwrks.IrcBot.Monad
    Clckwrks.IrcBot.Page.IrcLog
    Clckwrks.IrcBot.Page.IrcLogs
    Clckwrks.IrcBot.Page.Reconnect
    Clckwrks.IrcBot.Page.Settings
    Clckwrks.IrcBot.Page.Template
    Clckwrks.IrcBot.Plugin
    Clckwrks.IrcBot.PreProcess
    Clckwrks.IrcBot.Route
    Clckwrks.IrcBot.Types
    Clckwrks.IrcBot.URL
    Paths_clckwrks_plugin_ircbot

  Build-depends:
    base                    < 5,
    acid-state             >= 0.12 && < 0.16,
    attoparsec             >= 0.10 && < 0.14,
    blaze-html             >= 0.5 && < 0.10,
    bytestring             >= 0.10 && < 0.11,
    clckwrks               >= 0.21 && < 0.25,
    containers             >= 0.4 && < 0.7,
    directory              >= 1.1 && < 1.4,
    filepath               >= 1.2 && < 1.5,
    happstack-server       >= 7.0 && < 7.6,
    happstack-hsp          >= 7.2 && < 7.4,
    hsp                    >= 0.9 && < 0.11,
    ircbot                 >= 0.6 && < 0.7,
    ixset                  >= 1.0 && < 1.2,
--    magic                  == 1.0.*,
    mtl                    >= 2.0 && < 2.3,
    network                >= 2.3 && < 2.9,
    reform                 == 0.2.*,
    reform-happstack       == 0.2.*,
    reform-hsp             >= 0.2 && < 0.3,
    safecopy               >= 0.6,
    text                   >= 0.11 && < 1.3,
    web-plugins            >= 0.1 && < 0.3,
    web-routes             == 0.27.*,
    web-routes-th          >= 0.21

