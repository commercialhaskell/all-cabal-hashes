name:
  miso-from-html
version:
  0.2.0.0
synopsis:
  Convert HTML to miso View syntax
description:
  HTML parser that pretty prints to a Miso View
bug-reports:
  https://github.com/dmjio/miso-from-html/issues
license:
  BSD3
license-file:
  LICENSE
author:
  David Johnson
maintainer:
  djohnson.m@gmail.com
copyright:
  (c) David Johnson 2020
category:
  Web
build-type:
  Simple
extra-source-files:
  README.md
  index.html
cabal-version:
  >=1.10

executable miso-from-html
  main-is:
    Main.hs
  ghc-options:
    -Wall
  build-depends:
     attoparsec
   , base < 5
   , bytestring
   , containers
   , pretty-simple
   , text
  default-language:
    Haskell2010

source-repository head
   type: git
   location: https://github.com/dmjio/miso-from-html.git