library
  default-language:    Haskell2010
  build-depends:       base >= 4 && < 5,
                       bytestring,
                       containers,
                       deepseq >= 1.1 && < 1.4,
                       ghc-prim,
                       mtl >= 1.1.1.0 && < 2.2,
                       parallel >= 2.2 && < 3.3,
                       parsec >= 2 && < 4,
                       terminfo >= 0.3 && < 0.4,
                       unix,
                       utf8-string >= 0.3 && < 0.4,
                       vector >= 0.7

  exposed-modules:     Graphics.Vty
                       Graphics.Vty.Attributes
                       Graphics.Vty.DisplayRegion
                       Graphics.Vty.Image
                       Graphics.Vty.Inline
                       Graphics.Vty.LLInput
                       Graphics.Vty.Picture
                       Graphics.Vty.Terminal
                       Codec.Binary.UTF8.Width

  other-modules:       Data.Marshalling
                       Data.Terminfo.Parse
                       Data.Terminfo.Eval
                       Graphics.Vty.Attributes.Color
                       Graphics.Vty.Attributes.Color240
