name:                vector-instances
version:             3.3.0.1
synopsis:            Orphan Instances for 'Data.Vector'
homepage:            http://github.com/ekmett/vector-instances
bug-reports:         http://github.com/ekmett/vector-instances/issues
license:             BSD3
license-file:        LICENSE
author:              Edward Kmett
maintainer:          ekmett@gmail.com
category:            Data, Data Structures
build-type:          Simple
cabal-version:       >=1.6
extra-source-files:
  .travis.yml
  .ghci
  .vim.custom
  .gitignore
  CHANGELOG.markdown
  README.markdown

source-repository head
  type: git
  location: git://github.com/ekmett/vector-instances.git

library
  other-extensions: TypeFamilies
  exposed-modules: Data.Vector.Instances
  hs-source-dirs: src
  build-depends:
    base          >= 4       && < 5,
    vector        >= 0.9     && < 0.11,
    semigroupoids >= 3,
    semigroups    >= 0.8.3.1,
    comonad       >= 3,
    pointed       >= 3,
    keys          >= 3


